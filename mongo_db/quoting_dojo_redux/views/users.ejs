<body>
	<h1>Here are the awesome quotes!</h1>
	<% for(var i=0;i<users.length;i++){ %>
	<div id = "users-information">
		<ul>
			<li>"<%= users[i].quote %>"</li>
			<li>-<%= users[i].name %> at <%= users[i].createdAt %></li>
		</ul>
	</div>
	<% } %>
</body>
</html>